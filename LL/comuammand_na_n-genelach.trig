     <#Erímon>
        a foaf:Person;
        irishRel:nomName "Erímon";
        rdfs:comment "Ic h-Erímon tra condrecat na teora Connachta. & h. Neill in tuascirt & h. Neill in descirt & Airgialla & na Dési & Laigin & Ossairge. & Éraind. & Orbraigi. & Fotharta. & Dal Riata. & Dal Fiatach. Et rigrad Alban. Et Síl Conaire & Cland Oengusa & Fergusa & Loairn tri mc h-Eirc. Et Síl Conaire i n-Albain & i nh-Erinn .i. Muscraigi & Corco Duibni.";
        owl:sameAs <http://example.com/LL/clanna_ébir_i_l-leith_chuind.trig#Erimon>;
        owl:sameAs <http://example.com/LL/lagin.trig#h-Erimoin>;
        irishRel:ancestorOfGroup <hNeillInTuascirt>, <hNeillInDescirt>, <#Airgialla>, <naDési>, <#Laigin>, <#Ossairge>, <#Éraind>, <#Orbraigi>, <#Fotharta>, <#DalRiata>, <#DalFiatach>, <#SílConaire>, <#SílConairein-Albain>, <#SílConaireinh-Erinn>, <#Muscraigi>, <#CorcoDuibni>.

     <hNeillInTuascirt>
        a irishRel:PopulationGroup ;
        irishRel:populationGroupName "h. Neill in tuascirt" .

     <hNeillInDescirt>
        a irishRel:PopulationGroup ;
        irishRel:populationGroupName "h. Neill in descirt" .

     <#Airgialla>
        a irishRel:PopulationGroup ;
        irishRel:populationGroupName "Airgialla" .

