     <#Éraind>
        a irishRel:PopulationGroup ;
        irishRel:populationGroupName "Éraind" .

     <#Orbraigi>
        a irishRel:PopulationGroup ;
        irishRel:populationGroupName "Orbraigi" .

     <#Fotharta>
