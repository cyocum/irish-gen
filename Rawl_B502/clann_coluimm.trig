@base <http://example.com/Rawl_B502/clann_coluimm.trig>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix foaf:  <http://xmlns.com/foaf/0.1/> .
@prefix rel: <http://purl.org/vocab/relationship/>.
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix dctype: <http://purl.org/dc/dcmitype/> .
@prefix irishRel: <http://example.com/earlyIrishRelationship.ttl#> .
@prefix prov: <http://www.w3.org/ns/prov#> .


<http://example.com/Rawl_B502> {
<>

    a dctype:Dataset;
    dcterms:title "Clann Coluimm"@sga;
    dcterms:isFormatOf <http://www.ucc.ie/celt/published/G105003/text023.html>;
    dcterms:format "application/trig" ;
    prov:asDerivedFrom <http://www.ucc.ie/celt/published/G105003/text023.html>.

<#Find>
    a foaf:Person;
    irishRel:nomName "Find";
    rel:hasParent <#Beirgín>.

<#Beirgín>
    a foaf:Person;
    irishRel:genName "Beirgín";
    rel:hasParent <#Diarmata>.

<#Diarmata>
    a foaf:Person;
    irishRel:genName "Diarmata";
    irishRel:nomName "Diarmaid";
    rel:hasParent <#Snéidgusa>.

<#Snéidgusa>
    a foaf:Person;
    irishRel:genName "Snéidgusa";
    irishRel:nomName "Snéidgus";
    rel:hasParent <#Fínáin>.

<#Fínáin>
    a foaf:Person;
    irishRel:genName "Fínáin";
    irishRel:genName "Fínán";
    rel:hasParent <#Brain>.

<#Brain>
    a foaf:Person;
    irishRel:genName "Brain" ;
    irishRel:nomName "Bran" ;
    rel:hasParent <#Coluim>.

<#Coluim>
    a foaf:Person ;
    irishRel:genName "Coluim" ;
    irishRel:nomName "Colum" ;
    rel:hasParent <#Rethach>.

<#Rethach>
    a foaf:Person;
    irishRel:genName "Rethach";
    rel:hasParent <#Ferbba>;
    owl:sameAs <http://example.com/Rawl_B502/genelach_ciarraige_luachra.trig#Rethech>.

<#Ferbba>
    a foaf:Person;
    irishRel:genName "Ferbba" ;
    irishRel:nomName "Ferbb" ;
    owl:sameAs <http://example.com/Rawl_B502/genelach_ciarraige_luachra.trig#Ferbba>.
}
